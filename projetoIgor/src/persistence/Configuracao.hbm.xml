<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="persistence.Configuracao" table="Configuracao">
		<id name="id" >
			<column name="id"/>
			<generator class="identity" />
		</id>
		<property name="linha" type="int">
			<column name="linha" />
		</property>
		<property name="coluna" type="int">
			<column name="coluna"/>
		</property>
		<property name="bomba" type="int">
			<column name="bomba"/>
		</property>
	</class>
</hibernate-mapping>